{
  "Comment": "A Step Function state machine that retrieves a file from a zip store, and posts a presigned url to download from.",
  "StartAt": "GetZipFile",
  "States": {
    "GetZipFile": {
      "Type": "Task",
      "Resource": "arn:aws:states:::lambda:invoke",
      "OutputPath": "$.Payload",
      "Parameters": {
        "Payload.$": "$",
        "FunctionName": ${unzip_file_arn}
      },
      "End": true
    }
  }
}